{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:tailwind": "concurrently 'next dev' 'npm run tailwind --watch'",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind": "tailwindcss -i styles/globals.css -o styles/dist.css",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "postinstall": "npm run tailwind"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "clsx": "1.2.1",
    "concurrently": "7.5.0",
    "next": "latest",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "6.0.0-beta.2",
    "styled-jsx": "5.1.0",
    "use-count-up": "3.0.1"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "postcss": "^8.4.19",
    "postcss-easings": "^3.0.1",
    "postcss-import": "^15.0.0",
    "postcss-import-url": "^7.0.0",
    "postcss-preset-env": "^7.8.3",
    "tailwindcss": "^3.2.4"
  }
}
